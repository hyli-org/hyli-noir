{
  "name": "hyli-noir",
  "description": "A library of noir programs used by Hyli.",
  "repository": {
    "type": "git",
    "url": "https://github.com/hyli-org/hyli-noir.git"
  },
  "bugs": {
    "url": "https://github.com/hyli-org/hyli-noir/issues"
  },
  "author": "Hyli",
  "license": "MIT",
  "version": "0.4.0",
  "type": "module",
  "main": "dist/hyli-noir.cjs.js",
  "module": "dist/hyli-noir.es.js",
  "exports": {
    "import": {
      "types": "./dist/lib.d.ts",
      "default": "./dist/hyli-noir.es.js"
    },
    "require": {
      "types": "./dist/lib.d.ts",
      "default": "./dist/hyli-noir.cjs.js"
    }
  },
  "types": "dist/lib.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "rm -rf dist && vite build && tsc && tsc-alias",
    "prepublishOnly": "bun run build",
    "pub": "npm publish"
  },
  "peerDependencies": {
    "@aztec/bb.js": "2.0.3",
    "@noir-lang/noir_js": "1.0.0-beta.18",
    "@noir-lang/noir_wasm": "1.0.0-beta.18"
  },
  "dependencies": {
    "hyli": "^0.4.1",
    "noir-jwt": "^0.4.5"
  },
  "devDependencies": {
    "@types/bun": "^1.3.9",
    "tsc-alias": "^1.8.16",
    "esbuild": "^0.20.2",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vite-bundle-analyzer": "^0.21.0"
  }
}
